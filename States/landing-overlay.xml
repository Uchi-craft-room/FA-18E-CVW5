<PropertyList>
  <name type="string" n="0">approach</name>
  <description type="string">Final approach to a runway.  Everything out - gear, flaps, speed-brakes</description>
  <overlay>
    <fdm>
      <yasim>
        <respect-external-gear-state>true</respect-external-gear-state>
      </yasim>
    </fdm>
    <controls>
      <engines>
        <engine n="0">
          <throttle>0.75</throttle>
        </engine>
        <engine n="1">
          <throttle>0.75</throttle>
        </engine>
      </engines>
      <flight>
        <elevator>-0.5</elevator>
        <flaps>1.0</flaps>
        <speedbrake>0.0</speedbrake>
      </flight>
      <gear>
        <tailhook>false</tailhook>
        <gear-down>true</gear-down>
        <brake-parking>0</brake-parking>
      </gear>
    </controls>
  </overlay>
</PropertyList>
